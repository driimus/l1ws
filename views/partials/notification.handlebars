
{{#if msg}}
<div class="row-12" id="notification">
	<div
		class="alert bg-info text-white alert-dismissible fade show col-xs-10 col-md-6 mx-auto"
		role="alert"
	>
		{{msg}}
		<button
			type="button"
			class="close"
			data-dismiss="alert"
			aria-label="Close"
		>
			<span aria-hidden="true">&times;</span>
		</button>
	</div>
</div>
{{/if}}
